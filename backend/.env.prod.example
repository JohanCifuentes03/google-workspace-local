# ========================================
# GOOGLE WORKSPACE MCP - PRODUCTION CONFIG
# ========================================

# Google OAuth Credentials (OBLIGATORIO)
GOOGLE_CLIENT_ID=your_production_client_id_here
GOOGLE_CLIENT_SECRET=your_production_client_secret_here

# Server Configuration
PUBLIC_URL=https://your-domain.com
PORT=3000
NODE_ENV=production

# Redis Configuration
REDIS_URL=redis://redis:6379

# Session Configuration
SESSION_TTL=86400
CLEANUP_INTERVAL=3600000

# Logging
LOG_LEVEL=warn

# Security (Production)
# TRUST_PROXY=true
# CORS_ORIGIN=https://your-domain.com

# Health Check
# HEALTH_CHECK_PATH=/status/test